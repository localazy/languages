{
  "name": "@localazy/languages",
  "version": "1.0.0",
  "description": "Available Localazy Languages",
  "type": "module",
  "types": "./dist/languages.d.ts",
  "module": "./dist/languages.js",
  "main": "./dist/languages.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/languages.d.ts",
        "default": "./dist/languages.js"
      },
      "require": {
        "types": "./dist/languages.d.ts",
        "default": "./dist/languages.js"
      }
    },
    "./data/*": "./dist/data/*.json",
    "./data/translations/*": "./dist/data/translations/*.json"
  },
  "scripts": {
    "build": "rimraf dist && npm run main-ts:build && npm run pre-build-scripts && vite build --mode production && npm run post-build-scripts",
    "main-ts:build": "reexport src/index.ts",
    "pre-build-scripts": "./scripts/pre-build/index.mjs",
    "post-build-scripts": "./scripts/post-build/index.mjs",
    "lint": "npm run lint:command",
    "lint:fix": "npm run lint:command -- --fix",
    "lint:command": "eslint --cache --cache-location .cache/eslint/ --cache-strategy content src",
    "prettier": "npm run prettier:command -- --check",
    "prettier:fix": "npm run prettier:command -- --write",
    "prettier:command": "prettier --cache --cache-location .cache/prettier/ --cache-strategy content src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/localazy/languages.git"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "typescript",
    "localazy",
    "languages"
  ],
  "engines": {
    "node": ">=16",
    "npm": ">=8.5"
  },
  "author": "Localazy",
  "homepage": "https://github.com/localazy/languages.git",
  "devDependencies": {
    "@localazy/cli": "^1.7.3",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^22.13.14",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-promise": "^6.1.1",
    "fs-extra": "^11.1.1",
    "prettier": "3.5.3",
    "reexport": "^1.4.1",
    "standard-version": "^9.5.0",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.28.0",
    "vite": "^6.2.4",
    "vite-plugin-dts": "^4.5.3"
  }
}
